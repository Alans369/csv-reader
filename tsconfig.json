{
  "compilerOptions": {
    "target": "ES2020",
    "allowImportingTsExtensions":true,
    "noEmit":true,
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    
  },
  "include": [
    "./**/*.ts"
  ]
}